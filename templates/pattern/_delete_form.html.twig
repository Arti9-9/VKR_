<form method="post" action="{{ path('app_pattern_delete', {'id': pattern.id}) }}" onsubmit="return confirm('Вы уверены чот хотите удалить данный элемент?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ pattern.id) }}">
    <button class="btn btn-danger">Удалить</button>
</form>
