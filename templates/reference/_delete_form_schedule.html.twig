<form method="post"
      action="{{ path('app_schedule_delete_user',{ 'schedule': auditorium[1], 'direction' : direction.id, 'curriculum': curriculum.id}) }}"
      onsubmit="return confirm('Вы уверены что хотите удалить аудиторию из расписания?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ auditorium[1]) }}">
    <p>{{ auditorium[0].Number }} - {{ auditorium[0].type }}
        <button class="btn btn-delete btn-circle btn-circle-sm m-1 bi bi-x">
        </button>
    </p>
    <p>Рабочих мест для студентов – {{ auditorium[0].CountSeats }}</p>

    </p>

</form>
